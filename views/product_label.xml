<odoo>

    <record id="product_template_view_form_inherit_label" model="ir.ui.view">
        <field name="name">product.template.common.form (label_fields)</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Etiquetas">
                    <field name="lote"/>
                    <field name="fecha_fab"/>
                    <field name="fecha_ven"/>
                    <field name="peso"/>
                    <field name="vida_util"/>
                    <field name="footer"/>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
